# Diffusion Service Configuration

# Model Selection
USE_SDXL=false  # true for SDXL (requires 12GB+ VRAM), false for SD 1.5 (requires 4-6GB VRAM)

# Model IDs (from Hugging Face)
SDXL_MODEL=stabilityai/stable-diffusion-xl-base-1.0
SD15_MODEL=runwayml/stable-diffusion-v1-5

# Performance Optimizations
ENABLE_ATTENTION_SLICING=true
ENABLE_VAE_SLICING=true

# Hugging Face Token (optional, only needed for gated models)
# HF_TOKEN=your_huggingface_token_here
